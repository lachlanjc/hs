{
  "trailingSlash": true,
  "github": { "silent": true },
  "rewrites": [
    {
      "source": "/astrocolony/:path*",
      "destination": "/astrocolony/site/:path*"
    },
    {
      "source": "/petit-prince-14/:path*",
      "destination": "/petit-prince-14/site/:path*"
    },
    {
      "source": "/sommeil/:path*",
      "destination": "/sommeil/site/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/astrocolony/",
      "destination": "/astrocolony/site/",
      "permanent": false
    },
    {
      "source": "/petit-prince-14/",
      "destination": "/petit-prince-14/site/",
      "permanent": false
    },
    {
      "source": "/sommeil/",
      "destination": "/sommeil/site/",
      "permanent": false
    },
  ]
}
